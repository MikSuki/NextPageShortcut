{
  "name": "NextPageShortcut",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "html/popup.html",
    "default_icon": {
      "128": "icon/icon.png"
    }
  }
}