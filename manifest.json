{
  "name": "NextPageShortcut",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "contextMenus",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "/js/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/js/content.js"]
    }
  ]
}